# COMSPEC Environment Variables Template
# Copy this file to .env.local and fill with real values

# Google Sheets Configuration
REACT_APP_GOOGLE_SHEETS_ID=your_google_sheets_id_here
REACT_APP_GOOGLE_SERVICE_ACCOUNT_EMAIL=your-service-account@project.iam.gserviceaccount.com
REACT_APP_GOOGLE_PRIVATE_KEY="your_private_key_here"
REACT_APP_GOOGLE_PROJECT_ID=your_project_id
REACT_APP_GOOGLE_CLIENT_ID=your_client_id

# Google Maps API
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Development Settings
REACT_APP_ENVIRONMENT=development
REACT_APP_DEBUG_MODE=true